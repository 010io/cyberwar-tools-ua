# Squad303 Environment Configuration
# Copy this file to .env and fill in your credentials
# DO NOT commit .env to Git!

# TextNow Credentials
TEXTNOW_USERNAME=your_username
TEXTNOW_SID=your_connect.sid_cookie
TEXTNOW_CSRF=your_csrf_token

# Webhook Security
WEBHOOK_SECRET=generate_random_32_char_string_here

# Rate Limiting (adjust based on your needs)
MAX_CALLS_PER_HOUR=30
MAX_CALLS_PER_DAY=80

# OPSEC (Operational Security)
USE_TOR=true
TOR_PROXY=socks5://127.0.0.1:9050

# Logging
LOG_LEVEL=INFO
LOG_FILE=squad303.log

# Flask Server
PORT=5000
FLASK_ENV=production
