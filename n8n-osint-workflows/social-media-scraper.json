{"name": "Social Media OSINT Scraper", "nodes": [{"name": "Trigger", "type": "n8n-nodes-base.trigger", "parameters": {}}, {"name": "Discord Webhook", "type": "n8n-nodes-base.discord", "parameters": {"webhookUrl": "={{$env.DISCORD_WEBHOOK}}", "text": "OSINT data received"}}, {"name": "Data Parser", "type": "n8n-nodes-base.code", "parameters": {"jsCode": "return $input.all().map(i => ({source: i.source, timestamp: new Date(), author: i.author, content: i.content}))"}}, {"name": "Rate Limiter", "type": "n8n-nodes-base.delay", "parameters": {"amount": 2}}, {"name": "Database Save", "type": "n8n-nodes-base.postgres", "parameters": {"query": "INSERT INTO osint_data VALUES ($1, $2, $3, $4)"}}], "connections": {"Trigger": {"main": [[{"node": "Data Parser", "type": "main", "index": 0}]]}, "Data Parser": {"main": [[{"node": "Rate Limiter", "type": "main", "index": 0}]]}, "Rate Limiter": {"main": [[{"node": "Database Save", "type": "main", "index": 0}]]}, "Database Save": {"main": [[{"node": "Discord Webhook", "type": "main", "index": 0}]]}}}
